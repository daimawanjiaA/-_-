.container {
  width: 100%;
  height: 100%;
}
.container .userinfo {
  color: red;
}
.index_swiper swiper {
  width: 750rpx;
  height: 400rpx;
}
.index_swiper swiper swiper-item image {
  width: 100%;
}
.index_bigType {
  padding-top: 20rpx;
  padding-left: 60rpx;
  background-color: #ffffff;
}
.index_bigType .bigTypeRow {
  display: flex;
}
.index_bigType .bigTypeRow view {
  width: 100%;
}
.index_bigType .bigTypeRow view navigator {
  flex: 1;
}
.index_bigType .bigTypeRow view navigator image {
  width: 150rpx;
}
.index_tip {
  padding-top: 20rpx;
  padding-left: 70rpx;
  background-color: #e4e3e3;
  height: 200rpx;
  display: flex;
}
.index_tip view {
  flex: 1;
}
.index_tip view image {
  width: 90rpx;
}
.index_tip view .ttip {
  display: flex;
  flex-direction: column;
  width: 140rpx;
  align-items: center;
}
.index_link1 {
  display: flex;
  padding-top: 20rpx;
  padding-bottom: 20rpx;
  padding-left: 40rpx;
  background-color: #e4e3e3;
  position: relative;
}
.index_link1 navigator {
  position: absolute;
  right: 70rpx;
  color: #ff0000;
  text-align: right;
}
.index_doctor {
  padding-top: 20rpx;
  padding-left: 30rpx;
  background-color: #ffffff;
}
.index_doctor .doctor {
  height: 300rpx;
  display: flex;
}
.index_doctor .doctor navigator {
  flex: 1;
}
.index_doctor .doctor navigator view {
  display: flex;
  flex-direction: column;
  align-items: center;
}
.index_doctor .doctor navigator view image {
  width: 150rpx;
}
